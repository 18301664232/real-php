body {
  background-color: #f0f1f4;
}
/*一级菜单的样式*/
.wtmenu {
  height: 60px;
  border-top: 1px solid #cbd2e1;
  border-bottom: 1px solid #cbd2e1;
  background-color: #ffffff;
}
.wtmenu .container {
  width: 1200px;
  height: 59px;
  background-color: #ffffff;
  border-bottom: 1px solid #cbd2e1;
}
.wtmenu .container > p {
  float: left;
  margin: 0;
  height: 58px;
  line-height: 58px;
  font-size: 18px;
  color: #0d2556;
}
.wtmenu .container > a {
  float:left;
  line-height:58px;
  margin-left:45px;
  font-size:14px;
  color:#a0abc2;
}
.wtmenu .container > a:hover {
  text-decoration:none;
}
.wtmenu .container > a:focus{
  text-decoration:none;
}
.wtmenu .container > .wtMyItemChoosed{
  color: #0d2556;
}
.wtmenu .container > .wtMyItem{
  margin-left:55px;
}
.wtmenu .container > .wtCreatePro{
  float: right;
  width: 128px;
  height: 34px;
  line-height: 32px;
  margin-top: 12px;
  border:1px solid #2ca972;
  background-color: #45ca90;
  padding: 0;
  color: #ffffff;
  font-size: 14px;
  background-image: url('../images/wtCreatePro.png');
  background-repeat: no-repeat;
  background-position: 20px 10px;
  text-indent: 48px;
  text-align: left;
}
.wtmenu .container > .wtCreatePro:hover{
  background-color:#58d59e;
}
/*全部项目的样式*/
.wtPro{
  display:none;
  width:1200px;
  overflow-x:hidden;
  margin:27px auto 0 auto;
}
.wtPro > .wtProMenu {
  width: 1200px;
}
.wtPro > .wtProMenu > .wtProMenuList {
  margin: 0 0 0 0;
  padding: 0 0 0 0;
  list-style-type: none;
}
.wtPro > .wtProMenu > .wtProMenuList > li {
  float: left;
  height: 30px;
  line-height: 30px;
}
.wtPro > .wtProMenu > .wtProMenuList > li > a {
  font-size: 12px;
  color: #8e9cb7;
}
.wtPro > .wtProMenu > .wtProMenuList > li.wtProMenuListItemChoosed > a {
  font-size: 12px;
  color: black;
}
.wtPro > .wtProMenu > .wtProMenuList > li > a:hover {
  text-decoration: none;
}
.wtPro > .wtProMenu > .wtProMenuList > li > a:focus {
  text-decoration: none;
}
.wtPro > .wtProMenu > .wtProMenuList > li + li {
  margin-left: 25px;
}
.wtPro > .wtProMenu > .wtProMenuList > .wtProMenuListOnline {
  margin-left: 40px;
}
.wtPro > .wtProMenu > .wtProMenuList > .wtProMenuListFlowPrompt > a {
  color: #4c67a0;
}
.wtPro > .wtProMenu > .wtProMenuList > .wtProSearchItem{
   float:right;
   border:1px solid #cbd2e1;
   border-radius:3px;
}
.wtPro > .wtProMenu > .wtProMenuList > .wtProSearchItem > .wtProSearch{
  float:left;
  width:127px;
  line-height:26px;
  border:1px solid #cbd2e1;
  color:#cacfda;
  font-size:12px;
  text-indent:15px;;
  border:none;
  border-top-left-radius:3px;
  border-bottom-left-radius:3px;
  background-color:#f9f9fc;
}
.wtPro > .wtProMenu > .wtProMenuList > .wtProSearchItem > .wtProSearch::-moz-placeholder{color:#cacfda;}
.wtPro > .wtProMenu > .wtProMenuList > .wtProSearchItem > .wtProSearch::-webkit-input-placeholder{color:#cacfda;}
.wtPro > .wtProMenu > .wtProMenuList > .wtProSearchItem > .wtProSearch:-ms-input-placeholder{color:#cacfda;}
.wtPro > .wtProMenu > .wtProMenuList > .wtProSearchItem > .wtProSeachBtn{
  float:left;
  height:28px;
  width:28px;
  background-image:url('../images/wtSearchBox.png');
  background-repeat:no-repeat;
  background-position:6px 8px;
  border:none;
  border-top-right-radius:3px;
  border-bottom-right-radius:3px;
  background-color:#f9f9fc;
}
.wtPro > .wtProBtn {
  display:none;
  width: 1200px;
  margin-top: 12px;
}
.wtPro > .wtProBtn > .wtProBtnList {
  list-style-type: none;
  margin: 0 0 0 0;
  padding: 0 0 0 0;
}
.wtPro > .wtProBtn > .wtProBtnList > li {
  float: left;
}
.wtPro > .wtProBtn > .wtProBtnList > li > button {
  height: 30px;
  color: #8e9cb7;
  font-size: 12px;
  width: 100px;
  border: 1px solid #cbd2e1;
  display:none;
}
.wtPro > .wtProBtn > .wtProBtnList > li.wtProBtnChoosed > button {
  color: #fff;
  background-color: #395999;
}
.wtPro > .wtProBtn > .wtProBtnList > .wtProBtnListAllChoose {
  margin-left: 20px;
}
.wtPro > .wtProBtn > .wtProBtnList > .wtProBtnListAllChoose > button {
