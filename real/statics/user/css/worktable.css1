/*反馈小滑块*/
.wtSlideBlock{
  position:fixed;
  top:-50px;
  z-index:1333;
  width:150px;
  height:50px;
  line-height:50px;
  background-color:rgba(0,0,0,0.6);
  opacity:0;
  -webkit-transition: opacity 300ms ease,top 300ms ease;
  -o-transition: opacity 300ms ease,top 300ms ease;
  transition: opacity 300ms ease,top 300ms ease;
  background-image:url('../images/wtSlideBlockImg.png');
  background-repeat:no-repeat;
  background-position:20px 15px;
  color:#fff;
  font-size:14px;
  text-indent:60px;
}


.wtSlideBlock.wtSlideBlockFalse{
  background-image:url('../images/wtSlideBlockImgFalse.png');
}
.wtSlideBlockState1{
  opacity:1;
  top:0px;
}
.wtSlideBlockState2{
  opacity:0;
  top:-50px;
}
