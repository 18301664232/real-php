  border: 1px solid #cbd2e1;
  padding: 0;
}
.wtPro > .wtProBtn > .wtProBtnList > .wtProBtnListAllChoose > button > span {
  float: left;
}
.wtPro > .wtProBtn > .wtProBtnList > .wtProBtnListAllChoose > button > span:nth-child(1) {
  width: 12px;
  height: 12px;
  border-radius: 6px;
  border: 1px solid #ccd3e2;
  background-color: #f0f1f4;
  margin-left: 10px;
  margin-top: 8px;
}
.wtPro > .wtProBtn > .wtProBtnList > .wtProBtnListAllChoose > button > span:nth-child(1) > .wtProBtnListAllChooseDot {
  display: none;
