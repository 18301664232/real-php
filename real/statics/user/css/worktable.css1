.loadBase{
  position:relative;
  width:2px;
  height:4px;
  /*background-color:blue;*/
  top:6px;
  left:7px;
}
.loadBase > div{
  position:absolute;
  width:2px;
  height:4px;
  background-color:#c2c2d9;
}
.loadBase > div:nth-child(1){
  transform:rotate(0deg)translateY(-6px);
  -webkit-transform:rotate(0deg)translateY(-6px);
}
.loadBase > div:nth-child(2){
  transform:rotate(45deg)translateY(-6px);
  -webkit-transform:rotate(45deg)translateY(-6px);
}
.loadBase > div:nth-child(3){
  transform:rotate(90deg)translateY(-6px);
  -webkit-transform:rotate(90deg)translateY(-6px);
}
.loadBase > div:nth-child(4){
  transform:rotate(135deg)translateY(-6px);
  -webkit-transform:rotate(135deg)translateY(-6px);
}
.loadBase > div:nth-child(5){
  transform:rotate(180deg)translateY(-6px);
  -webkit-transform:rotate(180deg)translateY(-6px);
}
.loadBase > div:nth-child(6){
  transform:rotate(225deg)translateY(-6px);
  -webkit-transform:rotate(225deg)translateY(-6px);
}
.loadBase > div:nth-child(7){
  transform:rotate(270deg)translateY(-6px);
  -webkit-transform:rotate(270deg)translateY(-6px);
}
.loadBase > div:nth-child(8){
  transform:rotate(315deg)translateY(-6px);
  -webkit-transform:rotate(315deg)translateY(-6px);
}
.loadBase > div:nth-child(9){
  transform:rotate(0deg)translateY(-6px);
  -webkit-transform:rotate(0deg)translateY(-6px);
  background-color:#f0f1f4;
  z-index:3;
